{
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "cSpell.userWords": [
      "Hostnames",
      "gitignore",
      "lbip",
      "mkdir",
      "netstandard",
      "rabbitmq",
      "repo",
      "symlinks",
      "terraform",
      "vimrc",
      "virtualbox",
      "whenke",
      "winstonhenke"
    ],
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "all",
    "files.exclude": {
      "**/.git": false,
      "**/.hg": false,
      "**/.svn": false,
      "**/CVS": false
    },
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.iconTheme": "vscode-icons",
    "vsicons.projectDetection.autoReload": true,
    "team.showWelcomeMessage": false,
    "terminal.integrated.shell.osx": "/usr/local/bin/bash",
    "workbench.editor.restoreViewState": false,
    "files.hotExit": "off",
    "window.newWindowDimensions": "maximized",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "files.autoGuessEncoding": true,
    "explorer.autoReveal": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "editor.suggest.statusBar.visible": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "editor.columnSelection": false,
    "window.title": "${rootName}",
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    },
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fill",
    "explorer.openEditors.visible": 20,
    "gitlens.views.compare.files.layout": "tree",
    "diffEditor.ignoreTrimWhitespace": false,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.preset": "OTBS",
    "editor.detectIndentation": false,
    "debug.console.wordWrap": false,
    "window.restoreWindows": "none",
    "editor.comments.insertSpace": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.proseWrap": "never",
    "search.exclude": {
      "**/.DS_Store": true,
      "**/.git": true
    },
    "salesforcedx-vscode-apex.java.home": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home"
}