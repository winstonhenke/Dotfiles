{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "beforeWhitespace",
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.wordWrap": "off"
  },
  "breadcrumbs.enabled": true,
  "codeAnalyzer.enableV5": true,
  "cSpell.diagnosticLevel": "Hint",
  "debug.console.wordWrap": false,
  "debug.openExplorerOnEnd": true,
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.detectIndentation": false,
  "editor.find.seedSearchStringFromSelection": "selection",
  "editor.fontSize": 13,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.inlineSuggest.enabled": false,
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fill",
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "explorer.openEditors.visible": 20,
  "files.associations": {
    "*.code-workspace": "json"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.git": false
  },
  "files.hotExit": "onExitAndWindowClose",
  "git-graph.commitDetailsView.fileView.fileTree.compactFolders": false,
  "git.allowForcePush": true,
  "git.autofetch": "all",
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.fetchOnPull": true,
  "git.ignoredRepositories": [
    "/opt/homebrew"
  ],
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "always",
  "git.rebaseWhenSync": true,
  "git.untrackedChanges": "separate",
  "github.copilot.enable": {
    "markdown": true
  },
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.defaultMergeMethod": "rebase",
  "githubPullRequests.notifications": "pullRequests",
  "githubPullRequests.pullBranch": "never",
  "gitlens.ai.generateCommitMessage.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.views.scm.grouped.hiddenViews": {
    "repositories": false
  },
  "gitlens.views.scm.grouped.views": {
    "launchpad": true,
    "repositories": true,
    "searchAndCompare": true
  },
  "javascript.format.semicolons": "insert",
  "javascript.suggest.completeFunctionCalls": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.updateLinksOnFileMove.enabled": "prompt",
  "markdownlint.focusMode": 2,
  "markdownlint.run": "onSave",
  "prettier.requireConfig": true,
  "prettier.useEditorConfig": false,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.languageServerMode": "full",
  "python.analysis.supportDocstringTemplate": true,
  "python.defaultInterpreterPath": "",
  "python.experiments.enabled": false,
  "salesforcedx-vscode-core.clearOutputTab": true,
  "salesforcedx-vscode-core.retrieve-test-code-coverage": true,
  "salesforcedx-vscode-core.show-cli-success-msg": false,
  "salesforcedx-vscode-core.telemetry.enabled": false,
  "salesforcedx-vscode-soql.experimental.validateQueries": true,
  "scm.alwaysShowRepositories": true,
  "scm.autoReveal": false,
  "scm.defaultViewMode": "tree",
  "search.exclude": {
    "**/.git": true,
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "bash",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.smoothScrolling": true,
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "none",
  "window.title": "${rootName}",
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.decorations.badges": false,
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.editor.openPositioning": "last",
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.tabActionLocation": "left",
  "workbench.iconTheme": "vscode-icons",
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.expandMode": "doubleClick",
  "xml.server.vmargs": "-Xmx1024M"
}